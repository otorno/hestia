{
  "name": "hestia",
  "version": "1.2.0",
  "description": "Federated Authentication for Blockstack",
  "scripts": {
    "lint": "eslint . --ext .ts,.vue",
    "test": "node build/backend/hestia.js",
    "start": "node build-prod/backend/hestia.js",
    "clean-backend": "trash build/{backend,common/util,common/static-serve}/*",
    "clean-backend:prod": "trash build-prod/{backend,common/util,common/static-serve}/*",
    "tsc-backend": "tsc -p tsconfig.backend.json",
    "tsc-backend:prod": "tsc -p tsconfig.backend.json --outDir ./build-prod",
    "cp-backend": "node scripts/copy-backend.js",
    "cp-backend:prod": "node scripts/copy-backend.js production",
    "build": "npm-run-all \"build:*\"",
    "build:backend": "npm-run-all clean-backend tsc-backend cp-backend",
    "build:frontend": "webpack --config webpack/frontend.config.js",
    "build-prod": "npm-run-all \"build-prod:*\"",
    "build-prod:backend": "npm-run-all clean-backend:prod tsc-backend:prod cp-backend:prod",
    "build-prod:frontend": "webpack --mode=production --config webpack/frontend.config.js"
  },
  "private": true,
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/filesize": "^5.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/helmet": "0.0.45",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/node": "^13.5.1",
    "@types/socket.io": "^2.1.4",
    "@types/sqlite3": "^3.1.6",
    "@types/uuid": "^3.4.7",
    "@types/yazl": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autoprefixer": "^9.7.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
